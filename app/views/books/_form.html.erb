        <h3>New book</h3>
        <%= form_with model: book_new do |f| %>
          <div class="newbook">
            <label for="inputTitle">Title</label><br>
            <%= f.text_field :title, id:"inputTitle" %>
          </div>
          <div class="newbook">
            <label for="inputBody">Opinion</label><br>
            <%= f.text_area :body, id:"inputBody" %>
          </div>
            <%= f.submit"Create Book", class:"btn btn-lg py-2 btn-success rounded mx-1" %>
        <% end %>